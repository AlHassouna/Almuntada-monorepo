{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "myworkspace",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "YTRhOTM1ZmYtZGEwMi00NzIyLTk1YWEtOWI1YjM2YmFjNWMxfHJlYWQtd3JpdGU="
      }
    }
  },
  "no-cloud": {
    "runner": "@nrwl/workspace/src/tasks-runner/default-tasks-runner",
    "options": {}
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json"
      ]
    },
    "test": {
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ]
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
